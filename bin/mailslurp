    operation="emptyInbox"
    ;;
    sendEmailSimple)
    operation="sendEmailSimple"
    ;;
    createContact)
    operation="createContact"
    ;;
    deleteContact)
    operation="deleteContact"
    ;;
    getAllContacts)
    operation="getAllContacts"
    ;;
    getContact)
    operation="getContact"
    ;;
